{
  "name": "ask-utils",
  "version": "2.3.2",
  "description": "utility functions for ask-sdk",
  "main": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "tsc": "tsc",
    "test": "jest",
    "lint": "eslint 'libs/**/*.{ts,tsx}'",
    "doc": "typedoc --out docs libs/",
    "postversion": "git push origin $(git describe --abbrev=0 --tags)",
    "test:watch": "jest --watch",
    "test:dev": "jest --watch --silent=false --verbose false --coverage"
  },
  "keywords": [
    "alexa",
    "ask-sdk",
    "alexa-skill"
  ],
  "bugs": {
    "url": "https://github.com/ask-utils/ask-utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ask-utils/ask-utils.git"
  },
  "author": "Hidetaka Okamoto <info@wp-kyoto.net> (https://wp-kyoto.net)",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@typescript-eslint/eslint-plugin": "^1.1.1",
    "@typescript-eslint/parser": "^1.1.1",
    "@typescript-eslint/typescript-estree": "^1.1.1",
    "ask-sdk": "^2.6.0",
    "ask-sdk-core": "^2.6.0",
    "ask-sdk-model": "^1.16.0",
    "aws-sdk": "^2.447.0",
    "eslint": "^5.12.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.1",
    "prettier": "^1.16.2",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "rollup": "^1.12.3",
    "rollup-plugin-typescript2": "^0.20.1",
    "ts-jest": "^23.10.5",
    "tslib": "^1.10.0",
    "typedoc": "^0.14.1",
    "typescript": "^3.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*/*.+(ts|tsx|js)"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  }
}
