{
  "name": "@ask-utils/situation",
  "version": "3.4.0",
  "description": "Handle skill situation for ask-sdk v2 session attributes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "rollup -c",
    "tsc": "tsc",
    "test": "jest",
    "lint": "eslint 'libs/**/*.{ts,tsx}'",
    "doc": "typedoc --out docs libs/",
    "test:watch": "jest --watch",
    "test:dev": "jest --watch --silent=false --verbose false --coverage"
  },
  "keywords": [],
  "author": "Hidetaka Okamoto <info@wp-kyoto.net> (https://wp-kyoto.net)",
  "license": "MIT",
  "devDependencies": {
    "@ask-utils/test": "^3.4.0",
    "rollup": "^1.16.7",
    "rollup-plugin-typescript2": "^0.21.2",
    "typescript": "^3.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ask-utils/core": "^3.4.0",
    "ask-sdk": "^2.8.0",
    "ask-sdk-core": "^2.8.0",
    "ask-sdk-model": "^1.28.0",
    "ask-sdk-runtime": "^2.8.0",
    "tslib": "^1.10.0"
  },
  "bugs": {
    "url": "https://github.com/ask-utils/ask-utils/issues"
  },
  "directories": {
    "lib": "libs",
    "test": "__tests__"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*/*.+(ts|tsx|js)",
      "**/__tests__/**/*.+(ts|tsx|js)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ask-utils/ask-utils.git"
  }
}
