{
  "name": "@ask-utils/service-client",
  "version": "3.0.4",
  "description": "alt service client for Alexa.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "rollup -c",
    "tsc": "tsc",
    "test": "jest",
    "lint": "eslint 'libs/**/*.{ts,tsx}'",
    "doc": "typedoc --out docs libs/",
    "test:watch": "jest --watch",
    "test:dev": "jest --watch --silent=false --verbose false --coverage"
  },
  "keywords": [
    "alexa",
    "ask-sdk",
    "alexa-skill"
  ],
  "private": true,
  "author": "Hidetaka Okamoto <info@wp-kyoto.net> (https://wp-kyoto.net)",
  "license": "MIT",
  "devDependencies": {
    "rollup": "^1.16.7",
    "rollup-plugin-typescript2": "^0.21.2",
    "typescript": "^3.5.3"
  },
  "directories": {
    "lib": "libs",
    "test": "__tests__"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*/*.+(ts|tsx|js)",
      "**/__tests__/**/*.+(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "ask-sdk": "^2.6.0",
    "ask-sdk-core": "^2.6.0",
    "ask-sdk-model": "^1.18.0",
    "ask-sdk-runtime": "^2.4.0",
    "axios": "^0.19.0",
    "tslib": "^1.10.0"
  },
  "bugs": {
    "url": "https://github.com/ask-utils/ask-utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ask-utils/ask-utils.git"
  }
}
