  "main": "dist/index.js",
  "scripts": {
    "test": "lerna run test",
    "type": "lerna run type",
    "build": "lerna run build",
    "bootstrap": "lerna bootstrap",
    "lerna": "lenra",
    "reboot": "lerna clean -y && lerna bootstrap",
    "postversion": "git push origin $(git describe --abbrev=0 --tags)",
    "lint": "eslint ./**/*.ts",
